# Required changes to build to incorporate into file executable
handlers += {
    'name' : 'alsa',
    'type' : 'audioManager',
    'sources' : ['system/audioManager/alsa/src/main.c'],
    'shouldTest' : false,
    'dependencies' : [
        {
            'name'          : 'libasound',
            'alternateName' : 'alsa'
        },
        {
            'name'          : 'libpthread',
            'alternateName' : 'pthread'
        },

    ]
}


